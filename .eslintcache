"../../lib/redux/reducers/user\";\n\nconst styles = {\n  valid: {\n    display: \"none\",\n  },\n  errors: {\n    color: \"crimson\",\n    display: \"block\",\n  },\n};\nfunction Checkout({ history }) {\n\n  const dispatch = useDispatch();\n  const { current } = useSelector((state) => state.user);\n\n  const [clientDetails, setClientDetails] = useState({\n    givenName: current?.givenName,\n    familyName: current?.familyName,\n    email: current?.email,\n  });\n\n  const [required, setRequired] = useState({\n    givenName: false,\n    familyName: false,\n    email: false,\n  });\n\n  const handleOnChange = (e) =>\n    setClientDetails((prevState) => ({\n      ...prevState,\n      [e.target.name]: e.target.value,\n    }));\n\n  const handleOnSubmit = (e) => {\n    e.preventDefault();\n    dispatch(updateUserProfile(clientDetails));\n    history.push(\"/payment\");\n  };\n\n  useEffect(() => {\n    setClientDetails({\n      givenName: current?.givenName,\n      familyName: current?.familyName,\n      email: current?.email,\n    });\n  }, [current]);\n\n  useEffect(() => {\n    setRequired({\n      givenName: !!clientDetails?.givenName?.length,\n      familyName: !!clientDetails?.familyName?.length,\n      email: !!clientDetails?.email?.length,\n    });\n  }, [clientDetails]);\n\n  const isValid = useMemo(() => {\n    let errors = [];\n    Object.entries(required).map(([key, value]) => {\n      if (!value) {\n        errors.push(key);\n      }\n    });\n    return !errors.length;\n  }, [required]);\n  return (\n    <section className=\"pt-5 pb-5\">\n      <div className=\"container\">\n        <div className=\"py-5 text-center row justify-content-center\">\n          <div className=\"col-md-10\">\n            <h2>Client Details :</h2>\n          </div>\n        </div>\n        <div className=\"row justify-content-center rounded shadow pt-5 pb-5 bg-white \">\n          <div className=\"col-md-8 \">\n            <form className=\"needs-validation\" onSubmit={handleOnSubmit}>\n              <div className=\"row\">\n                <div className=\"col-md-6 mb-3\">\n                  <label for=\"firstName\">First name</label>\n                  <input\n                    className=\"form-control\"\n                    type=\"text\"\n                    name=\"givenName\"\n                    id=\"firstName\"\n                    placeholder=\"please enter first name\"\n                    value={clientDetails.givenName}\n                    onChange={handleOnChange}\n                  />\n                  <small\n                    style={\n                      !!clientDetails.givenName ? styles.valid : styles.errors\n                    }\n                  >\n                    Valid first name is required.\n                  </small>\n                </div>\n\n                <div className=\"col-md-6 mb-3\">\n                  <label for=\"lastName\">Last name</label>\n                  <input\n                    className=\"form-control\"\n                    type=\"text\"\n                    name=\"familyName\"\n                    id=\"lastName\"\n                    placeholder=\"please enter last name\"\n                    value={clientDetails.familyName}\n                    onChange={handleOnChange}\n                  />\n                  <small\n                    style={\n                      !!clientDetails.familyName ? styles.valid : styles.errors\n                    }\n                  >\n                    Valid last name is required.\n                  </small>\n                </div>\n              </div>\n\n              <div className=\"mb-3\">\n                <label for=\"email\">\n                  Email <span className=\"text-muted\">(Optional)</span>\n                </label>\n                <input\n                  className=\"form-control\"\n                  type=\"email\"\n                  name=\"email\"\n                  id=\"email\"\n                  placeholder=\"you@example.com\"\n                  value={clientDetails.email}\n                  onChange={handleOnChange}\n                />\n                <small\n                  style={!!clientDetails.email ? styles.valid : styles.errors}\n                >\n                  Please enter a valid email address for order updates\n                </small>\n              </div>\n\n              <button\n                className=\"btn btn-primary btn-lg btn-block\"\n                type=\"submit\"\n                disabled={!isValid}\n              >\n                Continue to checkout\n              </button>\n            </form>\n          </div>\n        </div>\n      </div>\n    </section>\n  );\n}\nexport default Checkout;\n",["128","129"],"/home/julia/Elements/programmation project/marketplace_/src/components/Payment/index.js",["130","131"],"/home/julia/Elements/programmation project/marketplace_/src/lib/redux/reducers/cart.js",[],"/home/julia/Elements/programmation project/marketplace_/src/lib/redux/reducers/user.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/Layout/Footer.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/components.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/Layout/Header.js",[],"/home/julia/Elements/programmation project/marketplace_/src/lib/queries.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/Cart/CartFooter.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/Orders/Row.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/Cart/Row.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/Home/Sidebar.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/Home/Gallery.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/Payment/utils.js",[],"/home/julia/Elements/programmation project/marketplace_/src/components/Pa